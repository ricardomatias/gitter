{
  "name": "gitter",
  "version": "0.1.0",
  "description": "Node-based sequencer for the web.",
  "scripts": {
    "start": "webpack-dev-server --port 8000 --open --progress --colors ./webpack.config.js",
    "build": "webpack --config ./webpack.config.js",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "sequencer"
  ],
  "author": "Philipp Fromme",
  "license": "MIT",
  "dependencies": {},
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "diagram-js": "https://github.com/bpmn-io/diagram-js.git",
    "exports-loader": "^0.6.4",
    "file-saver": "^1.3.3",
    "gh-pages": "^0.12.0",
    "ids": "^0.2.0",
    "imports-loader": "^0.7.1",
    "jsmidgen": "^0.1.5",
    "lodash": "^4.17.4",
    "min-dom": "^0.2.0",
    "p5": "^0.5.11",
    "tiny-svg": "^0.1.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
}
